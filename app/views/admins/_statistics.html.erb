<div class="progress" style="margin-bottom:10px;margin-top:10px;">
		
			<div class="progress-bar progress-bar-striped" role="progressbar" style="width:
									<% percent =  (@users.count/(@users.count + @partners.count).to_f ) %>
									<%= percent.round(2)*100 %>%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100">Total Users <%= percent.round(2)*100 %>% </div>
			<div class="progress-bar progress-bar-striped bg-danger " role="progressbar" style="width:
									<% percent =  (@partners.count/(@users.count + @partners.count).to_f ) %>
									<%= percent.round(2)*100 %>%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100">Total Partners <%= percent.round(2)*100 %>% </div>
									</div>
			

		<div class="progress" style="margin-bottom:10px">
			<div class="progress-bar progress-bar-striped bg-info" role="progressbar" style="width: 
				<% a = 0 %>
				<% @users.each do |user| %>
						<% if user.status%>
							<% a += 1 %>
						<% end %>
				<% end %>
			<% b = ( a / (@users.count.to_f) ) %> 
			<%= b.round(2)*100 %>%" 
			aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">Active Users <%= b.round(2)*100 %>% </div>
		</div>
		<div class="progress" style="margin-bottom:10px">
			<div class="progress-bar progress-bar-striped bg-warning" role="progressbar" style="width: 100%" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100">Online Users 0%</div>
		</div>
		<div class="progress" style="margin-bottom:10px">
			<div class="progress-bar progress-bar-striped bg-danger" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">Waiting Requests 0%</div>
</div>